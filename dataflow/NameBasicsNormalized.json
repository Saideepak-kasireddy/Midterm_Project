{
	"name": "NameBasicsNormalized",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Snowflake_CleanNameBasics",
						"type": "DatasetReference"
					},
					"name": "NameBasicsClean"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ParquetNameBasics",
						"type": "DatasetReference"
					},
					"name": "NameBasicsFlatten"
				}
			],
			"transformations": [
				{
					"name": "Clean"
				},
				{
					"name": "PrimaryProfessionNormalize"
				}
			],
			"scriptLines": [
				"parameters{",
				"     Job_ID as string (\"10\")",
				"}",
				"source(output(",
				"          NCONST as string,",
				"          PRIMARYNAME as string,",
				"          BIRTHYEAR as string,",
				"          DEATHYEAR as string,",
				"          PRIMARYPROFESSION as string,",
				"          KNOWNFORTITLES as string,",
				"          JOB_ID as string,",
				"          CREATED_DATE as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table') ~> NameBasicsClean",
				"NameBasicsClean derive(JOB_ID = $Job_ID,",
				"          CREATED_DATE = toString(currentDate(),'yyyy-MM-dd'),",
				"          PRIMARYPROFESSION = split(PRIMARYPROFESSION, \",\")) ~> Clean",
				"Clean foldDown(unroll(PRIMARYPROFESSION),",
				"     mapColumn(",
				"          NCONST,",
				"          PRIMARYNAME,",
				"          BIRTHYEAR,",
				"          DEATHYEAR,",
				"          PRIMARYPROFESSION,",
				"          KNOWNFORTITLES,",
				"          JOB_ID,",
				"          CREATED_DATE",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> PrimaryProfessionNormalize",
				"PrimaryProfessionNormalize sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     partitionFileNames:['NameBasicsNormalized.parquet'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          NCONST,",
				"          PRIMARYNAME,",
				"          BIRTHYEAR,",
				"          DEATHYEAR,",
				"          PRIMARYPROFESSION,",
				"          KNOWNFORTITLES,",
				"          JOB_ID,",
				"          CREATED_DATE",
				"     ),",
				"     partitionBy('hash', 1)) ~> NameBasicsFlatten"
			]
		}
	}
}