{
	"name": "DF_LoadDimLanguage",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Snowflake_TitleAkas",
						"type": "DatasetReference"
					},
					"name": "CleanStageTitleAkas"
				},
				{
					"dataset": {
						"referenceName": "Snowflake_DimLanguage",
						"type": "DatasetReference"
					},
					"name": "DimLanguageMaxID"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "Snowflake_DimLanguage",
						"type": "DatasetReference"
					},
					"name": "sinkDimLanguage"
				}
			],
			"transformations": [
				{
					"name": "DimLanguagewithSK"
				},
				{
					"name": "DerivedColumn"
				},
				{
					"name": "surrogateKey"
				}
			],
			"scriptLines": [
				"parameters{",
				"     JOB_ID as string (\"10\")",
				"}",
				"source(output(",
				"          TITLEID as string,",
				"          ORDERING as decimal(38,0),",
				"          TITLE as string,",
				"          REGION as string,",
				"          LANGUAGE as string,",
				"          TYPES as string,",
				"          ATTRIBUTES as string,",
				"          ISORIGINALTITLE as boolean,",
				"          JOB_ID as string,",
				"          CREATED_DATE as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table') ~> CleanStageTitleAkas",
				"source(output(",
				"          LANGUAGE_SK as decimal(20,0)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     query: '  SELECT NVL(MAX(LANGUAGE_SK),0) AS LANGUAGE_SK FROM DAMG_SCHEMA.DIM_LANGUAGE',",
				"     format: 'query') ~> DimLanguageMaxID",
				"CleanStageTitleAkas, DimLanguageMaxID join(1==1,",
				"     joinType:'cross',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> DimLanguagewithSK",
				"surrogateKey derive(JOB_ID = $JOB_ID,",
				"          CREATED_DATE = currentTimestamp(),",
				"          LANGUAGE_SK = RowNum+LANGUAGE_SK) ~> DerivedColumn",
				"DimLanguagewithSK keyGenerate(output(RowNum as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey",
				"DerivedColumn sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     stageInsert: true,",
				"     mapColumn(",
				"          LANGUAGE_CODE = LANGUAGE,",
				"          JOB_ID,",
				"          CREATED_DATE,",
				"          LANGUAGE_SK",
				"     )) ~> sinkDimLanguage"
			]
		}
	}
}